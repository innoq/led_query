<% concepts_by_dimension.each_with_index do |(dim, concepts), i| %>
    <%= render.call "_dimension_query", {
            :dimension => "<#{dim}>",
            :include_descendants => include_descendants,
            :var => i
    } %>
    ?concept<%= i %> skos:exactMatch* ?match<%= i %> .
    FILTER(?match<%= i %> IN (<%= concepts %>))
<% end %>
